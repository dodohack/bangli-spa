    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "ngrx-store-freeze": "^0.1.9",
    "ngrx-store-logger": "^0.2.0",
    "node-sass": "^4.5.3",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.2",
    "sass-loader": "^6.0.6",
